{
    "name": "kimlonglecis/storemaintenance",
    "type": "magento-module",
    "description": "Store Maintenance allows you to put each store view separately into maintenance mode. It can be configured from backend, so no need to hack any of your files (e.g. index.php) on your server",
    "require": {
        "magento-hackathon/magento-composer-installer": "*"
    },
    "authors": [
        {
            "name": "Kim Long Le"
        }
    ],
    "extra": {
      "map" : [
          ["app/code/community/BalkeTechnologies/StoreMaintenance", "app/code/community/BalkeTechnologies/StoreMaintenance"],
          ["app/design/adminhtml/base/default/template/balketechnologies/storemaintenance/system/config/iparea.phtml", "app/design/adminhtml/base/default/template/balketechnologies/storemaintenance/system/config/iparea.phtml"],
          ["app/etc/modules/BalkeTechnologies_StoreMaintenance.xml", "app/etc/modules/BalkeTechnologies_StoreMaintenance.xml"],
          ["misc/default.html", "misc/default.html"],
          ["misc/prototype.js", "misc/prototype.js"],
          ["misc/timer.js", "misc/timer.js"],
          ["skin/frontend/base/default/balketechnologies/storemaintenance", "skin/frontend/base/default/balketechnologies/storemaintenance"],
          ["skin/frontend/default/default/balketechnologies/storemaintenance", "skin/frontend/default/default/balketechnologies/storemaintenance"]
      ]
    }
}
